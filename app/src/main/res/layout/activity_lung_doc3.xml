<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/lungback"
    tools:context=".lungs.lung_doc3">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="347dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="59dp"
        android:layout_marginEnd="201dp"
        android:transitionName="imagetransaction3"
        app:srcCompat="@drawable/lung_pic3" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView5"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-200dp"
        android:layout_marginTop="109dp"
        android:layout_marginBottom="270dp"
        android:layout_toEndOf="@+id/imageView5"
        android:foregroundGravity="center"
        android:gravity="center"
        android:text="@string/dr_emad_eddin"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        android:transitionName="nametransaction3" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView6"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-200dp"
        android:layout_marginTop="-250dp"
        android:layout_marginEnd="20dp"
        android:layout_toEndOf="@+id/imageView5"
        android:text="@string/professor_of_pulmonary_medicine_and_allergy_faculty_of_medicine_alexandria_university_consultant_bronchoscopy_and_thoracic_intensive_care"
        android:textColor="@color/gray"
        android:textSize="16sp"
        android:transitionName="desctransaction3" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_below="@+id/imageView5"
        android:layout_alignParentStart="true"
        android:layout_marginStart="27dp"
        android:layout_marginTop="33dp"
        app:srcCompat="@drawable/call_icon" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_below="@+id/imageView"
        android:layout_alignParentStart="true"
        android:layout_marginStart="27dp"
        android:layout_marginTop="33dp"
        app:srcCompat="@drawable/location_icon" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_below="@+id/imageView7"
        android:layout_alignParentStart="true"
        android:layout_marginStart="27dp"
        android:layout_marginTop="33dp"
        app:srcCompat="@drawable/money_icon" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView7"
        android:layout_marginStart="23dp"
        android:layout_marginTop="40dp"
        android:layout_toEndOf="@+id/imageView8"
        android:text="@string/_250_egp"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="normal" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="23dp"

        android:layout_marginTop="19dp"
        android:layout_marginEnd="20dp"
        android:layout_toEndOf="@+id/imageView7"
        android:text="@string/_8_college_of_medicine_street_doctors_tower_raml_station_raml_station_alexandria"
        android:textColor="@color/black"
        android:textStyle="normal" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView5"
        android:layout_marginStart="23dp"
        android:layout_marginTop="40dp"
        android:layout_toEndOf="@+id/imageView"
        android:text="@string/_4855593"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="normal" />



</RelativeLayout>