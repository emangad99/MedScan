<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bback"
    tools:context=".kidneys.shared_doc3">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="267dp"
        android:layout_height="475dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="-27dp"
        android:layout_marginEnd="160dp"
        android:transitionName="imagetransaction3"
        app:srcCompat="@drawable/pic3" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView5"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-195dp"
        android:layout_marginTop="109dp"
        android:layout_marginBottom="270dp"
        android:layout_toEndOf="@+id/imageView5"
        android:foregroundGravity="center"
        android:gravity="center"
        android:text="@string/dr_samar_el_sayed"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        android:transitionName="nametransaction3" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="145dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView6"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-165dp"
        android:layout_marginTop="-260dp"
        android:layout_marginEnd="25dp"
        android:layout_toEndOf="@+id/imageView5"
        android:text="@string/internist_specialized_in_adult_cardiology_pediatric_cardiology_adult_gastroenterology_and_endoscopy_adult_kidney_adult_liver_adult_diabetes_and_endocrinology_general_internal_medicine"
        android:textColor="@color/gray"
        android:textSize="16sp"
        android:transitionName="desctransaction3" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_below="@+id/imageView5"
        android:layout_alignParentStart="true"
        android:layout_marginStart="27dp"
        android:layout_marginTop="0dp"
        app:srcCompat="@drawable/call_icon" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_below="@+id/imageView"
        android:layout_alignParentStart="true"
        android:layout_marginStart="27dp"
        android:layout_marginTop="33dp"
        app:srcCompat="@drawable/location_icon" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_below="@+id/imageView7"
        android:layout_alignParentStart="true"
        android:layout_marginStart="27dp"
        android:layout_marginTop="33dp"
        app:srcCompat="@drawable/money_icon" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView7"
        android:layout_marginStart="23dp"
        android:layout_marginTop="40dp"
        android:layout_toEndOf="@+id/imageView8"
        android:text="@string/_100_egp"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="normal" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView"
        android:layout_marginStart="23dp"

        android:layout_marginTop="40dp"
        android:layout_toEndOf="@+id/imageView7"
        android:text="@string/agami_alex_matrouh_road"
        android:textColor="@color/black"
        android:textStyle="normal" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView5"
        android:layout_marginStart="23dp"
        android:layout_marginTop="7dp"
        android:layout_toEndOf="@+id/imageView"
        android:text="@string/_01204748392"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="normal" />

</RelativeLayout>